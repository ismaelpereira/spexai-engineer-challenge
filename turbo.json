{
   "$schema": "https://turborepo.dev/schema.json",
   "tasks": {
      "build": {
         "dependsOn": ["^build"],
         "outputs": ["dist/**"]
      },
      "check-types": {
         "dependsOn": ["^check-types"]
      },
      "db:generate": {
         "cache": false,
         "outputs": ["packages/database/generated/**"],
         "inputs": ["schema/**", "prisma.config.ts"]
      },
      "migrate:dev": {
         "cache": false
      },
      "dev": {
         "persistent": true,
         "cache": false
      },
      "test": {},
      "test:e2e": {}
   }
}
