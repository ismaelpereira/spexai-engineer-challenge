{
   "packageManager": "npm@11.4.2",
   "workspaces": [
      "apps/*",
      "apps/*/*",
      "packages/*"
   ],
   "scripts": {
      "dev": "turbo run dev",
      "dev:file-processor": "turbo run dev --filter=file-processor",
      "test:file-processor": "turbo run test --filter=file-processor",
      "test:e2e:file-api": "turbo run test:e2e --filter=file-api",
      "dev:file-api": "turbo run dev --filter=file-api",
      "test:e2e:file-processor": "turbo run test:e2e --filter=file-processor",
      "test:file-api": "turbo run test --filter=file-api",
      "build": "turbo run build",
      "lint": "turbo run lint",
      "test": "turbo run test",
      "db:generate": "turbo run db:generate",
      "db:migrate": "turbo run migrate:dev",
      "db:push": "cd packages/prisma && npm run db:push",
      "docker:build": "docker-compose build",
      "docker:up": "docker-compose up --build",
      "docker:down": "docker-compose down -v",
      "docker:logs": "docker-compose logs -f"
   },
   "dependencies": {
      "@prisma/client": "^7.3.0",
      "dotenv": "^17.2.4",
      "prisma": "^7.3.0"
   },
   "devDependencies": {
      "turbo": "^2.8.3"
   }
}
